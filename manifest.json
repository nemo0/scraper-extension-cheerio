{
  "manifest_version": 2,
  "name": "Scraper Again",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["*://*.facebook.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {},
  "permissions": ["tabs"]
}
